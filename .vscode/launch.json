  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug start",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles":["${workspaceFolder}/build/**/*.js"],
      "env": {"NODE_ENV": "debug"},
      "program": "${workspaceFolder}/build/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug seed",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {"NODE_ENV": "seed"},
      "program": "${workspaceFolder}/build/index.js"
    }
  ]
}